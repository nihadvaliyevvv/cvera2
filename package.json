{
  "name": "lastcvera",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma migrate deploy && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:backup": "pg_dump $DATABASE_URL > backup-$(date +%Y%m%d-%H%M%S).sql",
    "db:restore": "psql $DATABASE_URL < backup.sql",
    "db:test": "node -e \"require('@prisma/client').PrismaClient().then(p => p.user.count().then(c => console.log('✅ Database connection successful. Users:', c)).catch(e => console.error('❌ Database connection failed:', e.message)))\"",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
