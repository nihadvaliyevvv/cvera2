<svg xmlns="http://www.w3.org/2000/svg" width="300" height="400" viewBox="0 0 300 400">
  <!-- Background with subtle texture -->
  <rect width="300" height="400" fill="#fafafa"/>
  <rect width="300" height="400" fill="url(#texturePattern)" opacity="0.03"/>
  
  <!-- Define patterns -->
  <defs>
    <pattern id="texturePattern" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
      <circle cx="10" cy="10" r="1" fill="#000000" opacity="0.1"/>
    </pattern>
    <linearGradient id="premiumGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a1a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2d2d2d;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="goldAccent" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#b8860b;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#ffd700;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#b8860b;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Luxurious header with dark theme -->
  <rect x="0" y="0" width="300" height="100" fill="url(#premiumGradient)"/>
  <rect x="0" y="95" width="300" height="5" fill="url(#goldAccent)"/>
  
  <!-- Executive name section -->
  <rect x="30" y="25" width="180" height="16" rx="2" fill="#ffffff"/>
  <rect x="30" y="48" width="120" height="10" rx="2" fill="#e5e5e5"/>
  <rect x="30" y="65" width="140" height="8" rx="2" fill="#d1d1d1"/>
  
  <!-- Professional photo placeholder -->
  <circle cx="250" cy="50" r="30" fill="#ffffff" opacity="0.15"/>
  <circle cx="250" cy="50" r="20" fill="#ffffff" opacity="0.8"/>
  <circle cx="250" cy="50" r="15" fill="url(#premiumGradient)"/>
  
  <!-- Executive summary section -->
  <rect x="20" y="120" width="120" height="12" rx="2" fill="#1a1a1a"/>
  <rect x="145" y="122" width="120" height="2" rx="1" fill="url(#goldAccent)"/>
  
  <rect x="20" y="145" width="260" height="6" rx="1" fill="#4a4a4a"/>
  <rect x="20" y="156" width="240" height="6" rx="1" fill="#4a4a4a"/>
  <rect x="20" y="167" width="200" height="6" rx="1" fill="#4a4a4a"/>
  
  <!-- Professional experience with premium styling -->
  <rect x="20" y="190" width="140" height="12" rx="2" fill="#1a1a1a"/>
  <rect x="165" y="192" width="100" height="2" rx="1" fill="url(#goldAccent)"/>
  
  <!-- Executive position 1 -->
  <rect x="30" y="215" width="160" height="10" rx="1" fill="#2d2d2d"/>
  <rect x="200" y="217" width="70" height="6" rx="1" fill="#666666"/>
  <rect x="30" y="230" width="110" height="7" rx="1" fill="#888888"/>
  <rect x="30" y="242" width="220" height="5" rx="1" fill="#aaaaaa"/>
  <rect x="30" y="252" width="190" height="5" rx="1" fill="#aaaaaa"/>
  
  <!-- Executive position 2 -->
  <rect x="30" y="270" width="150" height="10" rx="1" fill="#2d2d2d"/>
  <rect x="190" y="272" width="80" height="6" rx="1" fill="#666666"/>
  <rect x="30" y="285" width="100" height="7" rx="1" fill="#888888"/>
  <rect x="30" y="297" width="210" height="5" rx="1" fill="#aaaaaa"/>
  
  <!-- Core competencies section -->
  <rect x="20" y="320" width="130" height="12" rx="2" fill="#1a1a1a"/>
  <rect x="155" y="322" width="110" height="2" rx="1" fill="url(#goldAccent)"/>
  
  <!-- Premium skill representation -->
  <rect x="30" y="345" width="80" height="8" rx="4" fill="#e0e0e0"/>
  <rect x="30" y="345" width="75" height="8" rx="4" fill="url(#goldAccent)"/>
  <text x="120" y="351" font-family="Arial, sans-serif" font-size="8" fill="#1a1a1a">Strategic Leadership</text>
  
  <rect x="30" y="360" width="80" height="8" rx="4" fill="#e0e0e0"/>
  <rect x="30" y="360" width="70" height="8" rx="4" fill="url(#goldAccent)"/>
  <text x="120" y="366" font-family="Arial, sans-serif" font-size="8" fill="#1a1a1a">Executive Management</text>
  
  <rect x="30" y="375" width="80" height="8" rx="4" fill="#e0e0e0"/>
  <rect x="30" y="375" width="78" height="8" rx="4" fill="url(#goldAccent)"/>
  <text x="120" y="381" font-family="Arial, sans-serif" font-size="8" fill="#1a1a1a">Board Relations</text>
  
  <!-- Luxury border design -->
  <rect x="0" y="0" width="300" height="400" fill="none" stroke="#d4af37" stroke-width="2"/>
  <rect x="3" y="3" width="294" height="394" fill="none" stroke="#1a1a1a" stroke-width="1"/>
  <rect x="6" y="6" width="288" height="388" fill="none" stroke="#e5e5e5" stroke-width="0.5"/>
  
  <!-- Premium badge -->
  <rect x="210" y="10" width="75" height="25" rx="12" fill="url(#goldAccent)"/>
  <text x="247.5" y="25" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#1a1a1a" font-weight="bold">PREMIUM</text>
  
  <!-- Executive accent elements -->
  <circle cx="15" cy="350" r="2" fill="url(#goldAccent)" opacity="0.8"/>
  <circle cx="285" cy="380" r="3" fill="url(#goldAccent)" opacity="0.6"/>
  <rect x="280" y="340" width="15" height="2" rx="1" fill="url(#goldAccent)" opacity="0.7"/>
  
  <!-- Sophisticated footer accent -->
  <rect x="0" y="395" width="300" height="5" fill="url(#goldAccent)" opacity="0.3"/>
</svg>
