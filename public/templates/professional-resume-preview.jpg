<!-- Professional Resume Template Preview - More realistic design -->
<svg width="300" height="400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Shadow filter -->
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="2" dy="4" stdDeviation="3" flood-opacity="0.3"/>
    </filter>
    
    <!-- Gradient for profile area -->
    <linearGradient id="sidebarGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#334155;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e293b;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background with shadow -->
  <rect width="300" height="400" fill="#ffffff" rx="8" filter="url(#shadow)"/>
  
  <!-- Sidebar with gradient -->
  <rect x="0" y="0" width="120" height="400" fill="url(#sidebarGradient)" rx="8 0 0 8"/>
  
  <!-- Profile Photo -->
  <circle cx="60" cy="50" r="22" fill="#ffffff" stroke="#e2e8f0" stroke-width="2"/>
  <text x="60" y="56" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#334155">JD</text>
  
  <!-- Name Section -->
  <text x="60" y="85" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" font-weight="bold" fill="#ffffff" letter-spacing="1px">JOHN DOE</text>
  <text x="60" y="100" text-anchor="middle" font-family="Arial, sans-serif" font-size="7" fill="#cbd5e1" letter-spacing="0.5px">SENIOR DEVELOPER</text>
  
  <!-- Contact Section -->
  <text x="15" y="125" font-family="Arial, sans-serif" font-size="8" font-weight="600" fill="#ffffff" letter-spacing="1px">CONTACT</text>
  <line x1="15" y1="130" x2="105" y2="130" stroke="#475569" stroke-width="1"/>
  
  <!-- Contact Items -->
  <circle cx="20" cy="142" r="2" fill="#94a3b8"/>
  <text x="28" y="146" font-family="Arial, sans-serif" font-size="6" fill="#cbd5e1">john@example.com</text>
  
  <circle cx="20" cy="155" r="2" fill="#94a3b8"/>
  <text x="28" y="159" font-family="Arial, sans-serif" font-size="6" fill="#cbd5e1">+1 234 567 890</text>
  
  <circle cx="20" cy="168" r="2" fill="#94a3b8"/>
  <text x="28" y="172" font-family="Arial, sans-serif" font-size="6" fill="#cbd5e1">New York, NY</text>
  
  <!-- Skills Section -->
  <text x="15" y="195" font-family="Arial, sans-serif" font-size="8" font-weight="600" fill="#ffffff" letter-spacing="1px">SKILLS</text>
  <line x1="15" y1="200" x2="105" y2="200" stroke="#475569" stroke-width="1"/>
  
  <!-- Skill Items -->
  <circle cx="18" cy="212" r="1.5" fill="#cbd5e1"/>
  <text x="25" y="216" font-family="Arial, sans-serif" font-size="6" fill="#cbd5e1">JavaScript</text>
  
  <circle cx="18" cy="225" r="1.5" fill="#cbd5e1"/>
  <text x="25" y="229" font-family="Arial, sans-serif" font-size="6" fill="#cbd5e1">React</text>
  
  <circle cx="18" cy="238" r="1.5" fill="#cbd5e1"/>
  <text x="25" y="242" font-family="Arial, sans-serif" font-size="6" fill="#cbd5e1">Node.js</text>
  
  <circle cx="18" cy="251" r="1.5" fill="#cbd5e1"/>
  <text x="25" y="255" font-family="Arial, sans-serif" font-size="6" fill="#cbd5e1">TypeScript</text>
  
  <!-- Languages Section -->
  <text x="15" y="280" font-family="Arial, sans-serif" font-size="8" font-weight="600" fill="#ffffff" letter-spacing="1px">LANGUAGES</text>
  <line x1="15" y1="285" x2="105" y2="285" stroke="#475569" stroke-width="1"/>
  
  <circle cx="18" cy="297" r="1.5" fill="#cbd5e1"/>
  <text x="25" y="301" font-family="Arial, sans-serif" font-size="6" fill="#cbd5e1">English (Native)</text>
  
  <circle cx="18" cy="310" r="1.5" fill="#cbd5e1"/>
  <text x="25" y="314" font-family="Arial, sans-serif" font-size="6" fill="#cbd5e1">Spanish (Professional)</text>
  
  <!-- Main Content Area -->
  <!-- Profile Section -->
  <text x="135" y="35" font-family="Arial, sans-serif" font-size="10" font-weight="600" fill="#334155" letter-spacing="1px">PROFILE</text>
  <line x1="135" y1="40" x2="285" y2="40" stroke="#cbd5e1" stroke-width="2"/>
  
  <rect x="135" y="48" width="140" height="3" fill="#64748b" opacity="0.6"/>
  <rect x="135" y="55" width="120" height="3" fill="#64748b" opacity="0.4"/>
  <rect x="135" y="62" width="135" height="3" fill="#64748b" opacity="0.4"/>
  
  <!-- Experience Section -->
  <text x="135" y="85" font-family="Arial, sans-serif" font-size="10" font-weight="600" fill="#334155" letter-spacing="1px">EXPERIENCE</text>
  <line x1="135" y1="90" x2="285" y2="90" stroke="#cbd5e1" stroke-width="2"/>
  
  <!-- Experience Item 1 -->
  <circle cx="140" cy="105" r="3" fill="#3b82f6"/>
  <line x1="140" y1="105" x2="140" y2="145" stroke="#cbd5e1" stroke-width="2"/>
  
  <text x="152" y="108" font-family="Arial, sans-serif" font-size="8" font-weight="600" fill="#334155">Senior Software Engineer</text>
  <text x="152" y="118" font-family="Arial, sans-serif" font-size="6" fill="#64748b" letter-spacing="0.5px">TECH CORP • SAN FRANCISCO</text>
  <text x="250" y="108" font-family="Arial, sans-serif" font-size="5" fill="#64748b">2020 - 2024</text>
  
  <rect x="152" y="125" width="110" height="2" fill="#64748b" opacity="0.4"/>
  <rect x="152" y="130" width="95" height="2" fill="#64748b" opacity="0.3"/>
  <rect x="152" y="135" width="105" height="2" fill="#64748b" opacity="0.3"/>
  
  <!-- Experience Item 2 -->
  <circle cx="140" cy="155" r="3" fill="#3b82f6"/>
  <line x1="140" y1="155" x2="140" y2="185" stroke="#cbd5e1" stroke-width="2"/>
  
  <text x="152" y="158" font-family="Arial, sans-serif" font-size="8" font-weight="600" fill="#334155">Software Developer</text>
  <text x="152" y="168" font-family="Arial, sans-serif" font-size="6" fill="#64748b" letter-spacing="0.5px">STARTUP INC • BOSTON</text>
  <text x="250" y="158" font-family="Arial, sans-serif" font-size="5" fill="#64748b">2018 - 2020</text>
  
  <rect x="152" y="175" width="100" height="2" fill="#64748b" opacity="0.4"/>
  <rect x="152" y="180" width="85" height="2" fill="#64748b" opacity="0.3"/>
  
  <!-- Education Section -->
  <text x="135" y="210" font-family="Arial, sans-serif" font-size="10" font-weight="600" fill="#334155" letter-spacing="1px">EDUCATION</text>
  <line x1="135" y1="215" x2="285" y2="215" stroke="#cbd5e1" stroke-width="2"/>
  
  <!-- Education Item -->
  <circle cx="140" cy="230" r="3" fill="#3b82f6"/>
  
  <text x="152" y="233" font-family="Arial, sans-serif" font-size="8" font-weight="600" fill="#334155">Bachelor of Computer Science</text>
  <text x="152" y="243" font-family="Arial, sans-serif" font-size="6" fill="#64748b" letter-spacing="0.5px">STANFORD UNIVERSITY</text>
  <text x="250" y="233" font-family="Arial, sans-serif" font-size="5" fill="#64748b">2016 - 2020</text>
  
  <!-- Template Label -->
  <rect x="0" y="370" width="300" height="30" fill="#f8fafc" opacity="0.9"/>
  <text x="150" y="385" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" font-weight="600" fill="#334155">Professional Resume</text>
  <text x="150" y="395" text-anchor="middle" font-family="Arial, sans-serif" font-size="7" fill="#64748b">Medium Tier • Sidebar Layout</text>
</svg>
